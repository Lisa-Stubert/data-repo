---
title: default
---
{% if dataset.lang %}
    {% assign locale = dataset.lang %}
{% elsif startpage.locale %}
    {% assign locale = startpage.locale %}
{% else %}
    {% assign locale = "de" %}
{% endif %}

{% if dataset.meta.meta[locale].title %}
    {% assign description = site[locale].metaDescription %}
    {% assign title = dataset.meta.meta[locale].title %}
{% else %}
    {% assign description = site[locale].metaDescription %}
    {% assign title = site[locale].metaTitle %}
{% endif %}
<!doctype html>
<html lang="{{ locale }}" itemscope itemtype="http://schema.org/Article">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>{{ title }} | ODIS</title>
        <meta name="description" content="{{ description }} | ODIS">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <link rel="apple-touch-icon" href="{{ site.baseUrl }}/assets/icons/icon.png">

        <link rel="icon" type="image/png" href="{{ site.baseUrl }}/assets/icons/favicon-alpha-32x32.png" sizes="32x32">
        <link rel="icon" type="image/png" href="{{ site.baseUrl }}/assets/icons/favicon-alpha-96x96.png" sizes="96x96">
        <link rel="shortcut icon" type="image/x-icon" href="{{ site.baseUrl }}/assets/favicon.ico">

        <meta name="generator" content="eleventy">
        <meta name="robots" content="index, follow">
        <meta http-equiv="content-language" content="de">

        <!-- FACEBOOK -->
        <meta property="fb:app_id" content="487094758334595">
        <meta property="og:url" content="{{ site.baseUrl + page.url }}">
        <meta property="og:type" content="website">
        <meta property="og:title" content="{{ title }} | ODIS">
        <meta property="og:image" content="{{ site.baseUrl }}/assets/icons/social_media.jpg">
        <meta property="og:description" content="{{ description }}">
        <meta property="og:site_name" content="{{ site[locale].metaTitle }} | ODIS">
        <meta property="article:author" content="Sebastian Meier">

        <!-- TWITTER -->
        <meta name="twitter:card" content="summary">
        <meta name="twitter:site" content="@TSBBerlin">
        <meta name="twitter:creator" content="@seb_meier">
        <meta name="twitter:url" content="{{ site.baseUrl + page.url }}">
        <meta name="twitter:title" content="{{ title }} | ODIS">
        <meta name="twitter:description" content="{{ description }}">
        <meta name="twitter:image" content="{{ site.baseUrl }}/assets/icons/social_media.jpg">

        <!-- GOOGLE+/SCHEMA.ORG -->
        <meta itemprop="name" content="{{ title }} | ODIS">
        <meta itemprop="description" content="{{ description }}">
        <meta itemprop="image" content="{{ site.baseUrl }}/assets/icons/social_media.jpg">

        <!-- CANONICAL URL -->
        <link rel="canonical" href="{{ site.baseUrl + page.url }}">

        <meta name="theme-color" content="#1e3791">
        <meta name="msapplication-TileColor" content="#ffffff">
        <meta name="msapplication-TileImage" content="{{ site.baseUrl }}/assets/icons/ms-icon-144x144.png">

        <link rel="stylesheet" href="{{ site.baseUrl }}/assets/css/style-lab.css">
        <link rel="stylesheet" href="{{ site.baseUrl }}/assets/css/style.css">
        <link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css" integrity="sha512-Rksm5RenBEKSKFjgI3a41vrjkw4EVPlJ3+OiI65vTjIdo9brlAacEuKOiQ5OFh7cOI1bkDwLqdLw3Zg0cRJAAQ==" crossorigin="">

      </head>
    <body>
        <nav>
            <div id="tsb-logo"><div class="inner">
                <div id="minilogo">
                    ODIS
                </div>
            </div></div>
            <div id="nav" class="container">
                <ul>
                    <li class="opendata"><a href="https://www.odis-berlin.de"><img src="{{ site.baseUrl }}/assets/images/opendata-icon@2x.png" alt="Daten" />{% if local == "de" %}back to ODIS{% else %}zurück zur ODIS{% endif %}</a></li>
                    <li class="lang"><a href="{{ site.baseUrl }}/{% if local == "de" %}en{% else %}de{% endif %}">{% if local == "de" %}English{% else %}Deutsch{% endif %}</a></li>
                </ul>
                <div id="burger"></div>
            </div>
        </nav>
        {{ content | safe }}
        <footer id="contact">
            <div id="footer">
                <div class="container">
                    <a href="http://www.technologiestiftung-berlin.de"><img src="{{ site.baseUrl }}/assets/images/tsb-logo.png" alt="Technologiestiftung Berlin" /></a>
                    <div class="contact-meta">
                        Grunewaldstraße 61-62<br />
                        10825 Berlin<br />
                        +49 30 209 69 99-0<br />
                        <a href="mailto:info@technologiestiftung-berlin.de">info@technologiestiftung-berlin.de</a>
                    </div>
                    <div class="contact-social">
                        <a href="https://twitter.com/tsbberlin"><img src="{{ site.baseUrl }}/assets/images/twitter.png" alt="Twitter"></a>
                        <a href="https://www.facebook.com/Technologiestiftung/"><img src="{{ site.baseUrl }}/assets/images/facebook.png" alt="Facebook"></a>
                    </div>
                </div>
            </div>
            <div id="end">
                <div class="container">
                    <div class="left">© Technologiestiftung Berlin 2017</div>
                    <div class="right">
                        <a href="https://www.technologiestiftung-berlin.de/de/stiftung/kontakt-anfahrt/">Kontakt</a>
                        &nbsp;|&nbsp;
                        <a href="https://www.technologiestiftung-berlin.de/de/impressum">Impressum</a>
                        &nbsp;|&nbsp;
                        <a href="https://www.technologiestiftung-berlin.de/de/datenschutz/">Datenschutz</a>
                    </div>
                </div>
            </div>
            <div id="funding-foot">
                <div id="funding-foot-inner">
                    <img src="{{ site.baseUrl }}/assets/images/tsb-foerderer--eu-landing.png" alt="Gefördert von der Senatsverwaltung für Wirtschaft, Energie und Betriebe und der Investitionsbank Berlin aus den Mitteln des Landes Berlin.">
                    <p>Gefördert von der Senatsverwaltung für Wirtschaft, Energie und Betriebe und der Investitionsbank Berlin aus den Mitteln des Landes Berlin.</p>
                </div>
            </div>
        </footer>
        <script src="https://d3js.org/d3.v4.min.js"></script>
        <script src="{{ site.baseUrl }}/assets/js/script.min.js"></script>
        <script src="https://unpkg.com/leaflet@1.3.1/dist/leaflet.js" integrity="sha512-/Nsx9X4HebavoBvEBuyp3I7od5tA0UzAxs+j83KgC8PU0kgB4XiK4Lfe4y4cgBtaRJQEIFCW+oC506aPT2L1zw==" crossorigin=""></script>
        <script type="text/javascript">

            if(d3.selectAll('#map').size()>=1){
                var map = L.map('map', {scrollWheelZoom: false}).setView([52.51, 13.42], 10);

                var osmUrl='https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png';
                var osmAttrib='Map data © <a href="http://openstreetmap.org">OpenStreetMap</a> contributors';
                var osm = new L.tileLayer(osmUrl, {attribution: osmAttrib, id:'osm'});
                osm.addTo(map);

                d3.json(d3.select('#map').node().getAttribute('data-geojson'), function(e, d){
                    L.geoJSON(d, {
                        style: function (feature) {
                            return {
                                stroke: true,
                                weight: 0.5,
                                color: '#000000',
                                fill:true,
                                fillColor:'#000000',
                                fillOpacity:0.3
                            };
                        }
                    }).bindPopup(function (layer) {
                        return JSON.stringify(layer.feature.properties);
                    }).addTo(map);
                });
            }

        </script>
        <!-- Piwik -->
        <script type="text/javascript">
          var _paq = _paq || [];
          _paq.push(['trackPageView']);
          _paq.push(['enableLinkTracking']);
          (function() {
            var u="//piwik.technologiestiftung-berlin.de/";
            _paq.push(['setTrackerUrl', u+'piwik.php']);
            _paq.push(['setSiteId', 5]);
            var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
            g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'piwik.js'; s.parentNode.insertBefore(g,s);
          })();
        </script>
        <noscript><p><img src="//piwik.technologiestiftung-berlin.de/piwik.php?idsite=5" style="border:0;" alt="" /></p></noscript>
        <!-- End Piwik Code -->
    </body>
</html>